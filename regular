beginfig(152);
z6=(3,8.5);
z8=(4,8.5);
z11=(3,9);
z12=(4,9);
z1=(2,7);
z2=(6,1);
draw z1{dir 210}..z2;
draw z2{dir 40}..z1;
draw z6--z11;
draw z8--z12;
endfig;

%U trema
